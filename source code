#include<iostream>
using namespace std;

class a
{
    public :
    int arr[10];

};

int main()
{
    int sum;
    a obj;
    
    for(int i = 0;i<=10;i++)
    {
        cout<<"enter the "<<i+1<<endl;
        cin>>obj.arr[i];
    }
    
    for(int i = 0;i<=10;i++)
    {
        sum = sum+obj.arr[i];
    }
    
    cout<<"sum of array "<<sum<<endl;
    
    return 0;

}
